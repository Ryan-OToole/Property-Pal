<%if @admin%>
    <%= render '/partials/admin_navigation' %>
<%elsif @tenant %>
    <%= render '/tenants/tenant_navigation'%>
<%else%>
    <%= render '/partials/navigation'%>
<%end%>
<h1>Properties:</h1>

<ul>
    <% @properties.each do |property| %>
        <li><%= link_to property.name, property %></li>
    <% end %>
</ul>
