<%= form_for @tenant do |f|%>
<%= f.label :email%>
<%= f.text_field :email%><br>
<%= f.label :password%>
<%= f.text_field :password%><br>
<%= f.label :hint_password%>
<%= f.text_field :hint_password%><br>
<%= f.label :first_name%>
<%= f.text_field :first_name%><br>
<%= f.label :last_name%>
<%= f.text_field :last_name%><br>
<%= f.label :address_id%>
<%= f.text_field :address_id%><br>
<%= f.label :unit_id%>
<%= f.text_field :unit_id%> <br>
<%= f.submit "Create an account"%> 
<%end%>